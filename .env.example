# Environment Variables for AWS Amplify
# Copy this file to .env.local and fill in your actual AWS credentials

# AWS Configuration (Authentication Disabled)
NEXT_PUBLIC_AWS_REGION=us-east-1

# API Gateway Endpoint (if using AWS backend)
NEXT_PUBLIC_API_ENDPOINT=https://your-api-gateway-url.execute-api.us-east-1.amazonaws.com/dev

# Authentication disabled - app works without login
NEXT_PUBLIC_ENABLE_AUTH=false
