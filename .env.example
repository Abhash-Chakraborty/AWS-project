# Environment Variables for AWS Amplify
# Copy this file to .env.local and fill in your actual AWS credentials

# AWS Cognito Configuration
NEXT_PUBLIC_USER_POOL_ID=us-east-1_XXXXXXXXX
NEXT_PUBLIC_USER_POOL_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
NEXT_PUBLIC_IDENTITY_POOL_ID=us-east-1:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
NEXT_PUBLIC_AWS_REGION=us-east-1

# API Gateway Endpoint
NEXT_PUBLIC_API_ENDPOINT=https://your-api-gateway-url.execute-api.us-east-1.amazonaws.com/dev

# Set to 'true' to enable authentication, 'false' to disable for development
NEXT_PUBLIC_ENABLE_AUTH=false
